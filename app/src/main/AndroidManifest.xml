        <activity
            android:name="logeshd.analysed.virtualRecruitment"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="logeshd.analysed.resumeSortingTool"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>
</manifest>