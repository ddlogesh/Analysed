<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#F7ECECEC"
    tools:context=".completeProfile">

    <ImageView
        android:id="@+id/bt_back"
        android:layout_width="@dimen/_20sdp"
        android:layout_height="@dimen/_30sdp"
        android:padding="@dimen/_5sdp"
        android:layout_marginLeft="@dimen/_10sdp"
        android:layout_marginTop="@dimen/_6sdp"
        android:src="@drawable/profile_back"/>

    <TextView
        android:id="@+id/tv_text1"
        android:layout_below="@+id/bt_back"
        android:layout_marginLeft="@dimen/_15sdp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Complete Process"
        android:textColor="#C9000000"
        android:textSize="@dimen/_18ssp"/>

    <LinearLayout
        android:layout_below="@+id/tv_text1"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <View
                    android:id="@+id/view1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginLeft="@dimen/_30sdp"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="#60BBB4B4"/>

                <RelativeLayout
                    android:id="@+id/layout0"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tv_progress1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="20"
                        android:textColor="#7E000000"
                        android:layout_centerHorizontal="true"
                        android:textSize="@dimen/_20sdp"/>

                    <RelativeLayout
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_below="@+id/tv_progress1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true">
                        <ImageView
                            android:layout_width="@dimen/_100sdp"
                            android:layout_height="@dimen/_100sdp"
                            android:layout_centerInParent="true"
                            android:padding="@dimen/_3sdp"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:src="@drawable/profile_blue_circle"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="1"
                            android:textSize="@dimen/_30ssp"
                            android:textColor="#ffffff"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/_8sdp"/>
                        <com.mikhaellopez.circularprogressbar.CircularProgressBar
                            android:id="@+id/progressBar"
                            android:layout_width="@dimen/_105sdp"
                            android:layout_height="@dimen/_105sdp"
                            app:cpb_progress="20"
                            app:cpb_progress_max="100"
                            app:cpb_background_progressbar_color="#00000000"
                            app:cpb_progressbar_color="#7498FE"
                            app:cpb_progressbar_width="@dimen/_6sdp"/>
                    </RelativeLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/_15sdp"
                    android:layout_toEndOf="@+id/layout0">
                    <TextView
                        android:id="@+id/tv_title1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Personal"
                        android:textSize="@dimen/_20sdp"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:textColor="#DC000000" />
                    <TextView
                        android:id="@+id/tv_msg1"
                        android:layout_width="@dimen/_140sdp"
                        android:layout_height="wrap_content"
                        android:text="Hello, please fill out the detail and proceed with the next one"
                        android:layout_below="@+id/tv_title1"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:textSize="@dimen/_12ssp"
                        android:textColor="#7E000000"/>
                    <RelativeLayout
                        android:id="@+id/layout1"
                        android:layout_below="@+id/tv_msg1"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_35sdp">
                        <ImageView
                            android:id="@+id/iv_details1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/profile_button_back1"/>
                        <TextView
                            android:id="@+id/tv_details1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Details"
                            android:layout_marginTop="@dimen/_6sdp"
                            android:textColor="#ffffff"
                            android:layout_centerHorizontal="true"
                            android:textSize="@dimen/_12ssp" />
                    </RelativeLayout>
                    <ImageView
                        android:id="@+id/iv_plus"
                        android:layout_below="@+id/tv_msg1"
                        android:layout_toEndOf="@+id/layout1"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:src="@drawable/profile_plus_button"/>
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <View
                    android:id="@+id/view2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginLeft="@dimen/_30sdp"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="#60BBB4B4"/>

                <RelativeLayout
                    android:id="@+id/layout02"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tv_progress2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="45"
                        android:textColor="#7E000000"
                        android:layout_centerHorizontal="true"
                        android:textSize="@dimen/_20sdp"/>

                    <RelativeLayout
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_below="@+id/tv_progress2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true">
                        <ImageView
                            android:layout_width="@dimen/_100sdp"
                            android:layout_height="@dimen/_100sdp"
                            android:layout_centerInParent="true"
                            android:padding="@dimen/_3sdp"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:src="@drawable/profile_blue_circle"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="2"
                            android:textSize="@dimen/_30ssp"
                            android:textColor="#ffffff"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/_8sdp"/>
                        <com.mikhaellopez.circularprogressbar.CircularProgressBar
                            android:id="@+id/progressBar1"
                            android:layout_width="@dimen/_105sdp"
                            android:layout_height="@dimen/_105sdp"
                            app:cpb_progress="45"
                            app:cpb_progress_max="100"
                            app:cpb_background_progressbar_color="#00000000"
                            app:cpb_progressbar_color="#7498FE"
                            app:cpb_progressbar_width="@dimen/_6sdp"/>
                    </RelativeLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/_15sdp"
                    android:layout_toEndOf="@+id/layout02">
                    <TextView
                        android:id="@+id/tv_title2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Education"
                        android:textSize="@dimen/_20sdp"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:textColor="#DC000000" />
                    <TextView
                        android:id="@+id/tv_msg2"
                        android:layout_width="@dimen/_140sdp"
                        android:layout_height="wrap_content"
                        android:text="Hello, please fill out the detail and proceed with the next one"
                        android:layout_below="@+id/tv_title2"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:textSize="@dimen/_12ssp"
                        android:textColor="#7E000000"/>
                    <RelativeLayout
                        android:id="@+id/layout03"
                        android:layout_below="@+id/tv_msg2"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_35sdp">
                        <ImageView
                            android:id="@+id/iv_details2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/profile_button_back1"/>
                        <TextView
                            android:id="@+id/tv_details2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Details"
                            android:layout_marginTop="@dimen/_6sdp"
                            android:textColor="#ffffff"
                            android:layout_centerHorizontal="true"
                            android:textSize="@dimen/_12ssp" />
                    </RelativeLayout>
                    <ImageView
                        android:id="@+id/iv_plus2"
                        android:layout_below="@+id/tv_msg2"
                        android:layout_toEndOf="@+id/layout03"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:src="@drawable/profile_plus_button"/>
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <View
                    android:id="@+id/view3"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginLeft="@dimen/_30sdp"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="#60BBB4B4"/>

                <RelativeLayout
                    android:id="@+id/layout04"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tv_progress3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="75"
                        android:textColor="#7E000000"
                        android:layout_centerHorizontal="true"
                        android:textSize="@dimen/_20sdp"/>

                    <RelativeLayout
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_below="@+id/tv_progress3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true">
                        <ImageView
                            android:layout_width="@dimen/_100sdp"
                            android:layout_height="@dimen/_100sdp"
                            android:layout_centerInParent="true"
                            android:padding="@dimen/_3sdp"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:src="@drawable/profile_blue_circle"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="3"
                            android:textSize="@dimen/_30ssp"
                            android:textColor="#ffffff"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/_8sdp"/>
                        <com.mikhaellopez.circularprogressbar.CircularProgressBar
                            android:id="@+id/progressBar3"
                            android:layout_width="@dimen/_105sdp"
                            android:layout_height="@dimen/_105sdp"
                            app:cpb_progress="75"
                            app:cpb_progress_max="100"
                            app:cpb_background_progressbar_color="#00000000"
                            app:cpb_progressbar_color="#7498FE"
                            app:cpb_progressbar_width="@dimen/_6sdp"/>
                    </RelativeLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/_15sdp"
                    android:layout_toEndOf="@+id/layout04">
                    <TextView
                        android:id="@+id/tv_title3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Profession"
                        android:textSize="@dimen/_20sdp"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:textColor="#DC000000" />
                    <TextView
                        android:id="@+id/tv_msg3"
                        android:layout_width="@dimen/_140sdp"
                        android:layout_height="wrap_content"
                        android:text="Hello, please fill out the detail and proceed with the next one"
                        android:layout_below="@+id/tv_title3"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:textSize="@dimen/_12ssp"
                        android:textColor="#7E000000"/>
                    <RelativeLayout
                        android:id="@+id/layout05"
                        android:layout_below="@+id/tv_msg3"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_35sdp">
                        <ImageView
                            android:id="@+id/iv_details3"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/profile_button_back1"/>
                        <TextView
                            android:id="@+id/tv_details3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Details"
                            android:layout_marginTop="@dimen/_6sdp"
                            android:textColor="#ffffff"
                            android:layout_centerHorizontal="true"
                            android:textSize="@dimen/_12ssp" />
                    </RelativeLayout>
                    <ImageView
                        android:id="@+id/iv_plus3"
                        android:layout_below="@+id/tv_msg3"
                        android:layout_toEndOf="@+id/layout05"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:src="@drawable/profile_plus_button"/>
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>