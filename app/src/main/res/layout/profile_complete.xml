<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f7ececec">

    <RelativeLayout
        android:id="@id/layout0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/bt_back"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_centerVertical="true"
            android:padding="@dimen/_7sdp"
            android:src="@drawable/ic_home" />

        <TextView
            android:id="@+id/tv_text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/bt_back"
            android:padding="@dimen/_5sdp"
            android:text="Complete Process"
            android:textColor="#ff000000"
            android:textSize="@dimen/_16ssp" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/layout0"
        android:layout_marginTop="@dimen/_8sdp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.0">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <View
                    android:id="@+id/view1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginLeft="@dimen/_30sdp"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="#60bbb4b4" />

                <RelativeLayout
                    android:id="@id/layout0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/_20sdp">

                    <TextView
                        android:id="@+id/tv_progress1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="20"
                        android:textColor="#7e000000"
                        android:textSize="@dimen/_20sdp" />

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_progress1"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="@dimen/_5sdp">

                        <ImageView
                            android:layout_width="@dimen/_100sdp"
                            android:layout_height="@dimen/_100sdp"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:padding="@dimen/_3sdp"
                            android:src="@drawable/profile_blue_circle" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:paddingBottom="@dimen/_5sdp"
                            android:text="1"
                            android:textColor="#ffffffff"
                            android:textSize="@dimen/_30ssp" />

                        <com.mikhaellopez.circularprogressbar.CircularProgressBar
                            android:id="@+id/progressBar"
                            android:layout_width="@dimen/_105sdp"
                            android:layout_height="@dimen/_105sdp"
                            app:cpb_background_progressbar_color="#00000000"
                            app:cpb_progress="20"
                            app:cpb_progress_max="100"
                            app:cpb_progressbar_color="#ff7498fe"
                            app:cpb_progressbar_width="@dimen/_6sdp" />
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/layout0"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/_15sdp">

                    <TextView
                        android:id="@+id/tv_title1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:text="Personal"
                        android:textColor="#dc000000"
                        android:textSize="@dimen/_20sdp" />

                    <TextView
                        android:id="@+id/tv_msg1"
                        android:layout_width="@dimen/_140sdp"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_title1"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:text="Hello, please fill out the detail and proceed with the next one"
                        android:textColor="#7e000000"
                        android:textSize="@dimen/_12ssp" />

                    <TextView
                        android:id="@id/tv_details1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_msg1"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:background="@drawable/button_blue_solid"
                        android:paddingLeft="@dimen/_10sdp"
                        android:paddingTop="@dimen/_1sdp"
                        android:paddingRight="@dimen/_10sdp"
                        android:paddingBottom="@dimen/_1sdp"
                        android:text="Details"
                        android:textColor="#ffffffff"
                        android:textSize="@dimen/_12ssp" />

                    <ImageView
                        android:id="@+id/iv_plus"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_below="@id/tv_msg1"
                        android:layout_toRightOf="@+id/tv_details1"
                        android:src="@drawable/profile_plus_button" />
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.0">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <View
                    android:id="@+id/view2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginLeft="@dimen/_30sdp"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="#60bbb4b4" />

                <RelativeLayout
                    android:id="@id/layout02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/_20sdp">

                    <TextView
                        android:id="@+id/tv_progress2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="45"
                        android:textColor="#7e000000"
                        android:textSize="@dimen/_20sdp" />

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_progress2"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="@dimen/_5sdp">

                        <ImageView
                            android:layout_width="@dimen/_100sdp"
                            android:layout_height="@dimen/_100sdp"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:padding="@dimen/_3sdp"
                            android:src="@drawable/profile_blue_circle" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:paddingBottom="@dimen/_5sdp"
                            android:text="2"
                            android:textColor="#ffffffff"
                            android:textSize="@dimen/_30ssp" />

                        <com.mikhaellopez.circularprogressbar.CircularProgressBar
                            android:id="@+id/progressBar1"
                            android:layout_width="@dimen/_105sdp"
                            android:layout_height="@dimen/_105sdp"
                            app:cpb_background_progressbar_color="#00000000"
                            app:cpb_progress="45"
                            app:cpb_progress_max="100"
                            app:cpb_progressbar_color="#ff7498fe"
                            app:cpb_progressbar_width="@dimen/_6sdp" />
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/layout02"
                    android:layout_marginLeft="@dimen/_15sdp">

                    <TextView
                        android:id="@+id/tv_title2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:text="Education"
                        android:textColor="#dc000000"
                        android:textSize="@dimen/_20sdp" />

                    <TextView
                        android:id="@+id/tv_msg2"
                        android:layout_width="@dimen/_140sdp"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_title2"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:text="Hello, please fill out the detail and proceed with the next one"
                        android:textColor="#7e000000"
                        android:textSize="@dimen/_12ssp" />

                    <TextView
                        android:id="@id/tv_details2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_msg2"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:background="@drawable/button_blue_solid"
                        android:paddingLeft="@dimen/_10sdp"
                        android:paddingTop="@dimen/_1sdp"
                        android:paddingRight="@dimen/_10sdp"
                        android:paddingBottom="@dimen/_1sdp"
                        android:text="Details"
                        android:textColor="#ffffffff"
                        android:textSize="@dimen/_12ssp" />

                    <ImageView
                        android:id="@+id/iv_plus2"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_below="@id/tv_msg2"
                        android:layout_toRightOf="@+id/tv_details2"
                        android:src="@drawable/profile_plus_button" />
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.0">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <View
                    android:id="@+id/view3"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginLeft="@dimen/_30sdp"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="#60bbb4b4" />

                <RelativeLayout
                    android:id="@id/layout04"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/_20sdp">

                    <TextView
                        android:id="@+id/tv_progress3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="75"
                        android:textColor="#7e000000"
                        android:textSize="@dimen/_20sdp" />

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_progress3"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="@dimen/_5sdp">

                        <ImageView
                            android:layout_width="@dimen/_100sdp"
                            android:layout_height="@dimen/_100sdp"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:padding="@dimen/_3sdp"
                            android:src="@drawable/profile_blue_circle" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:paddingBottom="@dimen/_5sdp"
                            android:text="3"
                            android:textColor="#ffffffff"
                            android:textSize="@dimen/_30ssp" />

                        <com.mikhaellopez.circularprogressbar.CircularProgressBar
                            android:id="@+id/progressBar3"
                            android:layout_width="@dimen/_105sdp"
                            android:layout_height="@dimen/_105sdp"
                            app:cpb_background_progressbar_color="#00000000"
                            app:cpb_progress="75"
                            app:cpb_progress_max="100"
                            app:cpb_progressbar_color="#ff7498fe"
                            app:cpb_progressbar_width="@dimen/_6sdp" />
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/layout04"
                    android:layout_marginLeft="@dimen/_15sdp">

                    <TextView
                        android:id="@+id/tv_title3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:text="Profession"
                        android:textColor="#dc000000"
                        android:textSize="@dimen/_20sdp" />

                    <TextView
                        android:id="@+id/tv_msg3"
                        android:layout_width="@dimen/_140sdp"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_title3"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:text="Hello, please fill out the detail and proceed with the next one"
                        android:textColor="#7e000000"
                        android:textSize="@dimen/_12ssp" />

                    <TextView
                        android:id="@id/tv_details3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_msg3"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:background="@drawable/button_blue_solid"
                        android:paddingLeft="@dimen/_10sdp"
                        android:paddingTop="@dimen/_1sdp"
                        android:paddingRight="@dimen/_10sdp"
                        android:paddingBottom="@dimen/_1sdp"
                        android:text="Details"
                        android:textColor="#ffffffff"
                        android:textSize="@dimen/_12ssp" />

                    <ImageView
                        android:id="@+id/iv_plus3"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_below="@id/tv_msg3"
                        android:layout_toRightOf="@+id/tv_details3"
                        android:src="@drawable/profile_plus_button" />
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>